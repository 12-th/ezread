<%- js("js/index.js") %>
<%- js("js/article.js") %>
<%- js('js/footer.js') %>
<%- js('js/sidebar.js') %>
<%- js('js/excerpt.js') %> 
<%- js('js/header.js') %> 
<%- css('css/index.css') %>
<%- css('css/article.css') %>
<%- css('css/footer.css') %>
<%- css('css/sidebar.css') %>
<%- css('css/excerpt.css') %> 
<%- css('css/header.css') %> 
<div class="sidebar"><%- partial('partial/sidebar') %></div>
<div class="index-grid">
    <div><%- partial('partial/header.ejs') %> </div>
    <div id="articles">
        <div></div>
        <div id="article-list">
            <% page.posts.each(function(post){ %>
                <% if(!post.excerpt) { %>
                <%- partial('partial/excerpt', {excerpt: post.content, tittle: post.title, url: post.path}) %>
                <% } else { %>
                <%- partial('partial/excerpt', {excerpt: post.excerpt, tittle: post.title, url: post.path}) %>
                <% } %>
            <%}) %>
        </div>
        <div></div>
    </div>
    <div id="pageinator"><%- paginator({
        prev_text: '<',
        next_text: '>',
        end_size: 3,
        mid_size: 5
      }) %></div>
    <div class="foot">
        <%- partial('partial/footer') %>
    </div>
</div>