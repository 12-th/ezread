
<% 
 let info ={posts:[], next:"", next_link:""};
    page.posts.forEach(function(post){
        info.posts.push({date: post.date.format("YYYY-MM-DD hh:mm:ss"),
                    title: post.title,
                    url: url_for(post.path)
                    });
    });
    info.next = page.next;
    info.next_link = page.next_link;
%> 
<xmp id="tag-info">
    <%- JSON.stringify(info) %> 
</xmp>